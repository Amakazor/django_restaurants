{% load static %}

<!DOCTYPE html>
<html lang="pl-PL">
    <head>
        <link rel="preconnect" href="https://fonts.googleapis.com">
        <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
        <link href="https://fonts.googleapis.com/css2?family=Source+Sans+Pro:ital,wght@0,200;0,300;0,400;0,600;0,700;0,900;1,200;1,300;1,400;1,600;1,700;1,900&display=swap" rel="stylesheet"> 

        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta name="description" content="Reviews of popular places to eat">
        <meta name="keywords" content="Review, Reviews, Restaurant, Restaurants, Food, Ranking">

        <title>{% block title %}Recenzje restauracji{% endblock title %}</title>
        <link rel="stylesheet" type="text/css" href="{% static 'css/main.min.css' %}"/>
        <link rel="stylesheet" type="text/css" href="{% static 'css/style.css' %}"/>

        <script src="{% static 'js/stylizedSelect.js' %}"></script>
        <script src="{% static 'js/sortSelectHandler.js' %}"></script>
        <script src="{% static 'js/hamburgerHandler.js' %}"></script>
    </head>

    <body>
        {% include "components/header.j2" %}
